ion: '',\n      snackbarTimeout: 3000\n    };\n  },\n  methods: {\n    loginClick: function loginClick() {\n      var _this = this;\n\n      if (this.loginName === '' || this.password === '') {\n        alert('账号或密码不能为空');\n      } else {\n        var userInfo = {\n          username: this.loginName,\n          password: this.password\n        };\n        this.$store.dispatch('user/LoginByEmail', userInfo).then(function () {\n          _this.snackbar = true;\n          _this.notification = '登录成功';\n          _this.snackbarColor = 'green';\n          setTimeout(function () {\n            _this.$router.push({\n              path: '/dashboard'\n            });\n          }, 1200);\n        }).catch(function (err) {\n          _this.snackbar = true;\n          _this.notification = '连接错误，请重试';\n          console.log(err);\n        });\n      }\n    },\n    registerClick: function registerClick() {\n      this.snackbar = true;\n      this.notification = '注册功能暂不能用，请联系管理员分配账号。可用的使试用账号为test，密码test';\n      this.snackbarTimeout = 5000;\n    },\n    jumpHome: function jumpHome() {\n      this.$router.push({\n        path: '/'\n      });\n    }\n  }\n};",{"version":3,"sources":["Login.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAwHA,eAAA;AACA,EAAA,IADA,kBACA;AACA,WAAA;AACA,MAAA,SAAA,EAAA,EADA;AAEA,MAAA,QAAA,EAAA,EAFA;AAGA,MAAA,WAAA,EAAA,EAHA;AAIA,MAAA,GAAA,EAAA,IAJA;AAKA,MAAA,YAAA,EAAA,IALA;AAMA,MAAA,QAAA,EAAA,KANA;AAOA,MAAA,aAAA,EAAA,EAPA;AAQA,MAAA,YAAA,EAAA,EARA;AASA,MAAA,eAAA,EAAA;AATA,KAAA;AAWA,GAbA;AAeA,EAAA,OAAA,EAAA;AACA,IAAA,UAAA,EAAA,sBAAA;AAAA;;AACA,UAAA,KAAA,SAAA,KAAA,EAAA,IAAA,KAAA,QAAA,KAAA,EAAA,EAAA;AACA,QAAA,KAAA,CAAA,WAAA,CAAA;AACA,OAFA,MAEA;AACA,YAAA,QAAA,GAAA;AAAA,UAAA,QAAA,EAAA,KAAA,SAAA;AAAA,UAAA,QAAA,EAAA,KAAA;AAAA,SAAA;AACA,aAAA,MAAA,CAAA,QAAA,CAAA,mBAAA,EAAA,QAAA,EAAA,IAAA,CAAA,YAAA;AACA,UAAA,KAAA,CAAA,QAAA,GAAA,IAAA;AACA,UAAA,KAAA,CAAA,YAAA,GAAA,MAAA;