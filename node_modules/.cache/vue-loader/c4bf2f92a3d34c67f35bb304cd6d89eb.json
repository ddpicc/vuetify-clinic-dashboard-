 'secondary'\n      },\n      elevation: {\n        type: [Number, String],\n        default: 10\n      },\n      inline: {\n        type: Boolean,\n        default: false\n      },\n      fullWidth: {\n        type: Boolean,\n        default: false\n      },\n      offset: {\n        type: [Number, String],\n        default: 0\n      },\n      title: {\n        type: String,\n        default: undefined\n      },\n      text: {\n        type: String,\n        default: undefined\n      }\n    },\n\n    computed: {\n      hasOffset () {\n        return this.$slots.header ||\n          this.$slots.offset ||\n          this.title ||\n          this.text\n      },\n      styles () {\n        if (!this.hasOffset) return null\n\n        return {\n          marginBottom: `${this.offset}px`,\n          marginTop: `${this.offset * 2}px`\n        }\n      }\n    }\n  }\n</script>\n"]}]}